<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.dreamheaven.companyannounce.domain.CompanyAnnouncement" table="T_announcement" schema="bmp">
        <id name="aid" type="java.lang.String">
            <column name="naid" length="20" />
            <generator class="native" />
        </id>
        <property name="companyId" type="java.lang.String">
            <column name="ncompanyId" length="20" />
        </property>
        <property name="annContent" type="java.lang.String">
            <column name="sannContent" length="1000" />
        </property>
    </class>
</hibernate-mapping>
