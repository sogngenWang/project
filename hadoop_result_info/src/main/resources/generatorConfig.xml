			connectionURL="jdbc:mysql://192.168.253.251:3306/hadoop_db?generateSimpleParameterMetadata=true"
			userId="root"
			password="admin@unicorn">
		</jdbcConnection>

		<javaTypeResolver >
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- generate Model -->
		<javaModelGenerator targetPackage="com.baidu.model" targetProject="E:\Workspaces\web\hadoop_result_info\src\main\java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		
		

		<!-- generate xml -->
