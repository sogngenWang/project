			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		
		<!-- generate Mapper -->
		<javaClientGenerator type="XMLMAPPER" targetPackage="com.baidu.dao"  targetProject="E:\Workspaces\web\hadoop_result_info\src\main\java">
