<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dreamheaven.domain.Ingredient" table="ingredient" schema="dinnerdb">
		<id name="ingredientId" column="ningredientid" type="java.lang.String">
			<generator class="native" />
		</id>
		<property name="ingredientName" type="java.lang.String">
			<column name="singredientname" />
		</property>
		<property name="dishId" type="java.lang.String">
			<column name="sdishid"  />
		</property>
		<property name="ingredientCash" type="java.lang.String">
			<column name="singredientcash" />
		</property>
	</class>
</hibernate-mapping>
