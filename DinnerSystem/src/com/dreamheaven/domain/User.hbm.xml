<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dreamheaven.domain.User" table="user" schema="dinnerdb">
		<id name="uid" column="nuid" type="java.lang.String">
			<generator class="native" />
		</id>
		<property name="username" type="java.lang.String">
			<column name="susername" length="20" />
		</property>
		<property name="password" type="java.lang.String">
			<column name="spassword" length="20" />
		</property>
		<property name="userType" type="java.lang.String">
			<column name="susertype" length="1" />
		</property>
	</class>
</hibernate-mapping>
