<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dreamheaven.domain.Dish" table="dish" schema="dinnerdb">
		<id name="dishId" column="NDISHID" type="java.lang.String">
			<generator class="native" />
		</id>
		<property name="dishName" type="java.lang.String">
			<column name="sdishname" />
		</property>
		<property name="classId" type="java.lang.String">
			<column name="sclassid"  />
		</property>
		<property name="dishCash" type="java.lang.String">
			<column name="sdishcash" />
		</property>
	</class>
</hibernate-mapping>
