<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dream.weddingexpo.bean.ProjectFile" table="projectfile" schema="merrysystem">
		<id name="fileId" type="java.lang.String">
			<column name="fileId"/>
			<generator class="native" />
		</id>
		<property name="type" type="java.lang.String">
			<column name="type" />
		</property>
		<property name="fileUrl" type="java.lang.String">
			<column name="fileUrl" />
		</property>
		<property name="mapUrl" type="java.lang.String">
			<column name="mapUrl" />
		</property>
	</class>
</hibernate-mapping>
