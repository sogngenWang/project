<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dream.weddingexpo.bean.Kinds" table="kinds"
		schema="MerrySystem">
		<id name="kindsId" type="java.lang.String">
			<column name="kindsId" length="20" />
		</id>
		<property name="kindsName" type="java.lang.String">
			<column name="kindsName" length="20" />
		</property>
		<property name="kindsStore" type="java.lang.String">
			<column name="kindsStore" length="20" />
		</property>
	</class>
</hibernate-mapping>
